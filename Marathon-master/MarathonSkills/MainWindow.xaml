<Window x:Class="MarathonSkills.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Style="{DynamicResource base_windw}">
    <Grid>
        <Rectangle Fill="{DynamicResource dark_grey}" Height="45" VerticalAlignment="Bottom"/>
        <Label x:Name="label_timer" Foreground="White" Content="{Binding Time, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" VerticalContentAlignment="Center" HorizontalAlignment="Center" Height="45" VerticalAlignment="Bottom"/>
        <Frame Content="Frame" Margin="0,0,0,45" HorizontalAlignment="Right" Width="694" Source="/MarathonSkills;component/MainPage.xaml" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
