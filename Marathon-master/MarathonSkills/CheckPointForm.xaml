            <Label Name="labelServiceName4" Foreground="Black" Content="Services" Margin="70,-40,42.2,0"/>
            <Image Name="pictureBoxServ5" Height="40" Width="40" Source="/Map/exbut.png" Margin="10,10,184.2,0" />
            <Label Name="labelServiceName5" Foreground="Black" Content="Services" Margin="70,-40,42.2,0"/>
