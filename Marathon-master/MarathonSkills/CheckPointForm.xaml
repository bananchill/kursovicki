            <Image HorizontalAlignment="Left" Height="35" VerticalAlignment="Top" Width="35" Source="/Map/checkpoint8.png" />
        </Button>
        <StackPanel Name="panel1" HorizontalAlignment="Right" Width="263" Margin="0,50,10,14.2">
            <Image Margin="226,5,0.2,0" Height="32" VerticalAlignment="Top" Source="/Map/exbut.png" />
            <Button Margin="0,-40,0,0" HorizontalAlignment="Right" Width="39" Click="button_Click_Close" Height="38" Opacity="0"/>
            <Label Name="labelTitle" Foreground="Black" Content="Name" Margin="0,-30,42,0"/>
