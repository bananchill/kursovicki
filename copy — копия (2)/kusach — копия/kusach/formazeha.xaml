<Window x:Class="kusach.formazeha"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kusach"
        mc:Ignorable="d"
        Title="formazeha" Height="450" Width="800">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FFF1F1F1" Offset="0"/>
            <GradientStop Color="#FF67B6DA" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <Image HorizontalAlignment="Left" Name="foto" Height="334" Opacity="0" VerticalAlignment="Top" Width="724"  Margin="26,54,0,0" />
        <ComboBox HorizontalAlignment="Left" Name="combo" Margin="26,27,0,0" VerticalAlignment="Top"  Width="120" SelectionChanged="ComboBox_SelectionChanged">

        </ComboBox>

    </Grid>
</Window>
