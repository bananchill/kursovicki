<Application x:Class="kusach.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:kusach"
             StartupUri="Login.xaml">
    <Application.Resources>
        <LinearGradientBrush x:Key="BackgroundWindowResource">
            <GradientStop Offset="0.00" Color="#8e402a"/>
            <GradientStop Offset="0.50" Color="#90ee90"/>
            <GradientStop Offset="1.00" Color="#FF299DEE"/>
        </LinearGradientBrush>
    </Application.Resources>
</Application>
